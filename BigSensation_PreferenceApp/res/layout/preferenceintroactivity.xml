<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <Button
        android:id="@+id/preferenceintroactivity_bt_newstart"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="새로 만들기"
    />
    <Button
        android:id="@+id/preferenceintroactivity_bt_nextstart"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="이어하기"
    />
    <Button
        android:id="@+id/preferenceintroactivity_bt_test"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="서버 테스트 "
    />
    <TextView 
        android:id="@+id/preferenceintroactivity_tv_test"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" 
    />

</LinearLayout>